<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="emcorp.studio.pijat"
    android:installLocation="preferExternal">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WebviewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainMassageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DetailMassageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VideoPlayerActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".TelungkupActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".TerlentangActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".TeknikActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>